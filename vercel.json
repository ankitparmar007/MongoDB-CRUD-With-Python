{
    "builds": [{
        "src": "/server/api.py",
        "use": "@now/python"
    }],
    "routes": [{
        "src": "/(.*)",
        "dest": "server/api.py",
        "headers": {
            "Access-Control-Allow-Origin": "*",
            "Access-Control-Allow-Credentials": "true",
            "Access-Control-Allow-Methods": "GET,POST, OPTIONS, PUT, DELETE, HEAD"
        }
    }]
}